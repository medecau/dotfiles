{
    "editor.largeFileOptimizations": false,
    "editor.inlineSuggest.enabled": true,
    "workbench.colorTheme": "Witch Hazel Hypercolor",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "gitlens.defaultDateFormat": "DD/MM/YYYY HH:mm",
    "gitlens.defaultDateShortFormat": "DD/MM/YYYY",
    "gitlens.defaultTimeFormat": "HH:mm",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "python.envFile": "${workspaceFolder}/.envrc",
    "python.formatting.provider": "black",
    "python.languageServer": "Pylance",
    "semgrep.languages": [
        "c",
        "go",
        "java",
        "javascript",
        "javascriptreact",
        "json",
        "ocaml",
        "php",
        "python",
        "ruby",
        "typescript",
        "typescriptreact"
    ],
    "python.linting.prospectorArgs": [
        "--no-autodetect",
        "--strictness=verylow"
    ],
    "python.linting.prospectorEnabled": true,
    "python.testing.pytestEnabled": true,
    "terminal.integrated.defaultProfile.osx": "zsh",
    "python.terminal.activateEnvInCurrentTerminal": true,
    "python.pythonPath": "${workspaceFolder}/.venv/bin/python",
    "editor.formatOnSave": true,
    "workbench.editor.highlightModifiedTabs": true,
    "vsintellicode.features.python.deepLearning": "enabled",
    "liveshare.focusBehavior": "prompt",
    "jupyter.askForKernelRestart": false,
    "liveshare.presence": true,
    "git.autofetch": true,
    "liveshare.featureSet": "insiders",
    "notebook.lineNumbers": "on",
    "notebook.diff.ignoreOutputs": true,
    "notebook.diff.ignoreMetadata": true,
    "notebook.consolidatedOutputButton": false,
    "workbench.preferredDarkColorTheme": "Witch Hazel",
    "workbench.preferredHighContrastColorTheme": "Witch Hazel Hypercolor",
    "workbench.preferredLightColorTheme": "Witch Hazel",
    "jupyter.interactiveWindowMode": "perFile",
    "editor.fontSize": 14,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "extensions.ignoreRecommendations": true,
    "redhat.telemetry.enabled": false,
    "python.formatting.blackPath": "~/bin/black",
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "python.testing.pytestEnabled": true,
    "gitlens.blame.ignoreWhitespace": true,
    "python.linting.banditPath": "~/bin/bandit",
    "python.linting.banditEnabled": true,
    "python.linting.mypyEnabled": true,
}