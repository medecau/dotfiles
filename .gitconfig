[filter "lfs"]
clean = git-lfs clean -- %f
smudge = git-lfs smudge -- %f
process = git-lfs filter-process
required = true

[user]
name = Pedro Rodrigues
email = medecau@gmail.com

[init]
defaultBranch = main

[alias]
alias = config --global --get-regexp "^alias"
br = branch
cm = commit

st = status -sb

spull = "!git stash && git pull --rebase && git stash pop"

co = checkout
new = "checkout -b"

update = "fetch --all --tags --prune"
