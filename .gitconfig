[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[user]
	name = Pedro Rodrigues
	email = medecau@gmail.com
[init]
	defaultBranch = main
[alias]
	br = branch
	cm = commit
	
	co = checkout
	new = "checkout -b"

	update = "fetch --all --tags --prune"
